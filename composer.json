{
    "name": "den-kuz/symfony-messenger-configurable-strategy",
    "description": "Configurable retry strategy for symfony/messenger component",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Kuznetsov",
            "email": "mixrus09@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Symfony\\Component\\Messenger\\Retry\\Configurable\\": "src" }
    },
    "require": {
        "php": ">=7.2",
        "symfony/messenger": "^4.3|^5.0|^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpstan/phpstan": "^1.5",
        "symplify/easy-ci": "^10.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2.5"
        }
    }
}
